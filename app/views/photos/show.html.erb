<% content_for :head do %>
    Photo <%= @photo.id %>
<% end %>
<div class="col-md-12"></div>
<div class="row"><%= image_tag(@photo.url, class: 'col-md-12') %></div>
<div class="row">
  <%= content_tag :div, @photo.title, class: 'col-md-6 photo_title' %>
  <%= content_tag :div, @photo.author, class: 'col-md-6 photo_author' %>
</div>
<div class="row">
  <div class="col-md-12">
    <%= link_to ' Edit', edit_photo_path(@photo), class: 'btn btn-warning fa fa-edit' %>
    <%= link_to ' Delete', @photo, method: :delete, class: 'btn btn-danger fa fa-trash' %>
    <%= link_to ' All Photos', photos_path, class: 'btn btn-primary fa fa-list' %>
  </div>
</div>